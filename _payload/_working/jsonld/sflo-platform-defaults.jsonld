{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "conf": "https://semantic-flow.github.io/ontology/sflo-config/",
    "sflo": "https://semantic-flow.github.io/ontology/semantic-flow-ontology/"
  },
  "@graph": [
    {
      "@id": "../../../",
      "@type": "KnopNodeConfig/",
      "rdfs:label": "sflo platform default configuration",
      "rdfs:comment": "The Semantic Flow platform defaultconfiguration should be used by applications to infer default behavior when not defined elsewhere.",
      "conf:versioningEnabled": true,
      "conf:useTimestamps": false,
      "conf:knopNodeConfigInheritanceEnabled": true,
      "conf:generateResourcePages": true,
      "conf:hasTemplateMappingSet": {
        "@id": "../../../platform-default-mapping-set/"
      }
    },
    {
      "@id": "../../../platform-default-mapping-set/",
      "@type": "TemplateMappingSet/",
      "rdfs:label": "sflo platform default template mappings",
      "rdfs:comment": "The Semantic Flow platform default template mappings should be used by applications to infer default template mappings when not defined elsewhere.",
      "conf:hasTemplateMapping": [
        {
          "@id": "../../../platform-default-mapping/"
        }
      ]
    },
    {
      "@id": "../../../platform-default-mapping/",
      "@type": "TemplateMapping/",
      "rdfs:label": "sflo platform default inner template mapping",
      "rdfs:comment": "The Semantic Flow platform default inner template mapping should be used by applications to infer default inner template mappings when not defined elsewhere.",
      "conf:innerTemplate": "../../../platform-inner-template.vto",
      "conf:outerTemplate": "../../../platform-outer-template.vto",
      "conf:stylesheet": "../../../platform.css"
    },
    {
      "@id": "sflo:ReferenceFlow/",
      "conf:folderToken/": "_ref-flow/",
      "conf:fileToken/": "_ref"
    },
    {
      "@id": "sflo:PayloadFlow/",
      "conf:folderToken/": "_payload-flow/",
      "conf:fileToken/": ""
    },
    {
      "@id": "sflo:KnopMetadataFlow/",
      "conf:folderToken/": "_knop/_meta-flow/",
      "conf:fileToken/": "_meta"
    },
    {
      "@id": "sflo:PointersDataset/",
      "conf:folderToken/": "_knop/_pointers/"
    },
    {
      "@id": "sflo:InventoryDataset/",
      "conf:folderToken/": "_knop/_inventory/"
    },
    {
      "@id": "sflo:OpslogDataset/",
      "conf:folderToken/": "_knop/_opslog/"
    },
    {
      "@id": "sflo:InheritableKnopConfig/",
      "conf:folderToken/": "_knop/_cfg-inh"
    },
    {
      "@id": "sflo:LocalKnopConfig",
      "conf:folderToken/": "_knop/_cfg-local"
    },
    {
      "@id": "sflo:BacklinksDataset/",
      "conf:folderToken/": "_knop/_backlinks/"
    },
    {
      "@id": "sflo:JsonldDistribution/",
      "conf:fileToken/": "jsonld"
    },
    {
      "@id": "sflo:TurtleDistribution/",
      "conf:fileToken/": "ttl"
    },
    {
      "@id": "sflo:TrigDistribution/",
      "conf:fileToken/": "trig"
    }
  ]
}
